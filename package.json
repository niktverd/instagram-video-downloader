{
  "name": "instagram-video-downloader",
  "version": "1.0.0",
  "description": "A React app to download user videos from instagram.",
  "main": "index.js",
  "_moduleAliases": {
    "#config": "dist/server/src/config",
    "#logic": "dist/server/src/logic/index.js",
    "#tests": "dist/server/src/tests",
    "#types": "dist/server/src/types/index.js",
    "#utils": "dist/server/src/utils/index.js",
    "#src": "dist/server/src",
    "$": "dist/server/src/sections"
  },
  "scripts": {
    "watch": "tsc-watch --onSuccess \"node dist/server --dev\"",
    "watch:port": "tsc-watch --onSuccess \"node dist/server --dev --port 8080\"",
    "dev": "npm run clean && npm run watch",
    "start": "node dist/server",
    "test": "NODE_ENV=production npm run build && node dist/server/src/tests/index.js",
    "build:compile": "tsc",
    "build": "npm run clean && npm run build:compile",
    "clean": "rm -rf dist/server",
    "lint": "run-p lint:js lint:prettier typecheck",
    "lint:fix": "run-s lint:js:fix lint:prettier:fix typecheck",
    "lint:js": "eslint '**/*.{js,ts}' --max-warnings=0",
    "lint:js:fix": "eslint '**/*.{js,ts}' --max-warnings=0 --fix",
    "lint:prettier": "prettier --check '**/*.{js,ts,json,yaml,yml,md}'",
    "lint:prettier:fix": "prettier --write '**/*.{js,ts,json,yaml,yml,md}'",
    "typecheck": "tsc -p . --noEmit",
    "docker:test": "docker build -t instagram-video-downloader-local . && docker run -p 8080:8080 --env-file .env instagram-video-downloader-local",
    "knip": "mkdir -p report && knip --reporter markdown | tee report/knip.md",
    "knip:check": "mkdir -p report && [ -f report/knip.md ] && cp report/knip.md report/knip.md.prev || touch report/knip.md.prev && npm run knip && diff -q report/knip.md report/knip.md.prev && echo '✅ No new knip issues detected' || (echo '❌ New knip issues detected! Fix them before committing.' && exit 1)",
    "dc": "npx depcruise --include-only \"^src\" --output-type dot src | dot -T svg > report/dependency-graph.svg",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --max-warnings=0",
      "prettier --write"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/pubsub": "^4.11.0",
    "@gravity-ui/eslint-config": "^2.1.1",
    "@gravity-ui/prettier-config": "^1.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "^8.42.0",
    "eslint-plugin-no-not-accumulator-reassign": "^0.1.0",
    "express": "^4.21.2",
    "firebase": "^11.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "formidable": "^3.5.4",
    "googleapis": "^144.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sharp": "^0.34.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/formidable": "^3.4.5",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.7",
    "dependency-cruiser": "^16.10.1",
    "husky": "^9.1.7",
    "knip": "^5.52.0",
    "lint-staged": "^15.5.1"
  }
}
